{
    "name": "diviky/code-analysis",
    "description": "grumphp config to automatically check conventions for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar.suda@gmail.com"
        }
    ],
    "require": {
        "phpro/grumphp": "^2.0",
        "phpmd/phpmd": "^2.9",
        "squizlabs/php_codesniffer": "^3.5",
        "fakerphp/faker": "^1.9",
        "orchestra/testbench": "^9.1|^10.1",
        "phpunit/phpunit": "^10.5|^11.2",
        "spatie/phpunit-snapshot-assertions": "^5.1",
        "spatie/test-time": "^1.2",
        "nunomaduro/collision": "^7.10|^8.1",
        "jasonmccreary/laravel-test-assertions": "^2.3",
        "mockery/mockery": "^1.4.2",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "jrmadsen67/laravel-route-coverage-test": "^1.1",
        "kitloong/laravel-migrations-generator": "^7.0",
        "thedoctor0/laravel-factory-generator": "^1.2",
        "orangehill/iseed": "^3.0",
        "staudenmeir/laravel-migration-views": "^1.6",
        "laravel/pint": "^1.13",
        "nunomaduro/phpinsights": "^2.4",
        "barryvdh/laravel-ide-helper": "^3.5",
        "spatie/laravel-ignition": "^2.0",
        "monolog/monolog": "^3.0",
        "symfony/console": "^6.0|^7.0",
        "yieldstudio/grumphp-laravel-pint": "^1.0",
        "psalm/plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest": "^3.0"
    },
    "require-dev": {
        "laravel/framework": "^11.22|^12.2"
    },
    "autoload": {},
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/diviky/code-analysis/grumphp.yml"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
