{
    "name": "diviky/code-analysis",
    "description": "grumphp config to automatically check conventions for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar.suda@gmail.com"
        }
    ],
    "require": {
        "brianium/paratest": "^6.3",
        "phpro/grumphp": "^1.0",
        "phpmd/phpmd": "^2.9",
        "squizlabs/php_codesniffer": "^3.5",
        "nunomaduro/larastan": "^1.0|^2.1",
        "beyondcode/laravel-self-diagnosis": "dev-master",
        "fakerphp/faker": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.8",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "spatie/test-time": "^1.2",
        "vimeo/psalm": "^4.3",
        "sebastian/phpcpd": "^6.0",
        "nunomaduro/collision": "^5.11|^6.1",
        "jasonmccreary/laravel-test-assertions": "^1.1",
        "mockery/mockery": "^1.4.2",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "palpalani/grumphp-rector-task": "^0.8.2",
        "beyondcode/laravel-query-detector": "^1.5",
        "jrmadsen67/laravel-route-coverage-test": "^1.1",
        "kitloong/laravel-migrations-generator": "^6.3",
        "thedoctor0/laravel-factory-generator": "^1.2",
        "mnabialek/laravel-sql-logger": "^2.2",
        "orangehill/iseed": "^3.0",
        "psalm/plugin-laravel": "^2.0",
        "staudenmeir/laravel-migration-views": "^1.5|^1.6",
        "laravel/pint": "^1.1",
        "nunomaduro/phpinsights": "^2.4"
    },
    "autoload": {},
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/diviky/code-analysis/grumphp.yml"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {},
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
